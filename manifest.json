{
  "manifest_version": 2,

  "name": "TamperQ",
  "description": "Replace HTTP headers and parameters",
  "version": "1.0",

  "browser_action": {
    "default_icon": "38.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}